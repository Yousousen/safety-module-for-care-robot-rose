import DataTypes.dzn;

interface IA {
	in void a();

	behaviour {
		enum State {Idle, Operating};
		State state = State.Idle;

		on a: { state = State.Operating; }
		on a: { state = State.Idle; }
	}
}

component A {
	provides IA iA;
	behaviour {
		enum State {Idle, Operating};
		State state = State.Idle;
		
		on iA.a(): { state = State.Operating; }
		on iA.a(): { state = State.Idle; }
	}
}